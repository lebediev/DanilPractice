package graf;

import java.util.HashSet;
import java.util.Set;

public class Uzel {

    private Set<Integer> arr = new HashSet<>();;

    public void setToArr(int id){
        arr.add(id);
    }

    public void delToArr(int id){
        arr.remove(id);
    }

    public Set<Integer> getArr(){
        return arr;
    }

}
