package graf;

import java.util.HashMap;
import java.util.HashSet;
import java.util.Map;
import java.util.Set;

public class Cont {

    Map<Integer, Uzel> cont = new HashMap<Integer, Uzel>();

    public void setToCont(Integer a, Uzel b){
        cont.put(a, b);
    }

    public Map<Integer, Uzel> getCont(){
        return cont;
    }
}
